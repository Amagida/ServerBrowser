<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.web.WebView?>

<TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
  <tabs>
    <Tab text="Screenshots">
         <content>
            <VBox prefHeight="200.0" prefWidth="100.0" spacing="5.0">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="10.0" top="5.0" />
               </padding>
               <children>
                  <HBox spacing="5.0">
                     <children>
                        <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#previousImage" text="Previous Image" HBox.hgrow="NEVER" />
                        <Button maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#nextImage" text="Next Image" HBox.hgrow="NEVER" />
                        <ComboBox fx:id="screenshotComboBox" maxWidth="1.7976931348623157E308" onAction="#chooseImage" HBox.hgrow="ALWAYS">
                           <HBox.margin>
                              <Insets left="20.0" />
                           </HBox.margin>
                        </ComboBox>
                     </children>
                  </HBox>
                  <StackPane fx:id="imageContainer" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
               </children>
            </VBox>
         </content></Tab>
    <Tab text="Saved Positions" />
      <Tab text="Chatlogs">
         <content>
            <VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <children>
                  <WebView fx:id="chatLogWebView" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS" />
                  <ButtonBar prefHeight="40.0" prefWidth="200.0">
                    <buttons>
                        <Button mnemonicParsing="false" onAction="#loadChatLog" text="Reload" />
                      <Button mnemonicParsing="false" onAction="#clearChatLog" text="Clear" />
                    </buttons>
                  </ButtonBar>
               </children>
            </VBox>
         </content></Tab>
  </tabs>
</TabPane>
