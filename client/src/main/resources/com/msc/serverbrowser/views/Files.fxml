<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<TabPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
	<tabs>
		<Tab text="Chatlogs">
			<content>
				<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
					<children>
						<TextArea fx:id="contentTextArea" editable="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS" />
						<ButtonBar prefHeight="40.0" prefWidth="200.0">
							<buttons>
								<Button mnemonicParsing="false" onAction="#loadChatLog" text="Reload" />
								<Button mnemonicParsing="false" onAction="#clearChatLog" text="Clear" />
							</buttons>
						</ButtonBar>
					</children>
				</VBox>
			</content>
		</Tab>
	</tabs>
</TabPane>
