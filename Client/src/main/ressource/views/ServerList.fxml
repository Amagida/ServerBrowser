					<PropertyValueFactory property="password" />
				</cellValueFactory>
			</TableColumn-->
			<TableColumn fx:id="columnHostname" minWidth="20.0" prefWidth="20.0" text="Hostname">
				<cellValueFactory>
					<PropertyValueFactory property="hostname" />
				</cellValueFactory>
			</TableColumn>
			<TableColumn fx:id="columnPlayers" maxWidth="66.0" minWidth="66.0" prefWidth="66.0" text="Players">
				<cellValueFactory>
