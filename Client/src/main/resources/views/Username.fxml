<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
	minHeight="-Infinity" minWidth="-Infinity" spacing="10.0"
	xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<HBox maxWidth="1.7976931348623157E308">
			<children>
				<Label maxWidth="1.7976931348623157E308" text="Username"
					HBox.hgrow="ALWAYS">
					<padding>
						<Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
					</padding>
					<font>
						<Font size="24.0" />
					</font>
				</Label>
				<TextField fx:id="usernameTextField" prefWidth="200.0">
					<HBox.margin>
						<Insets top="15.0" />
					</HBox.margin>
				</TextField>
			</children>
			<padding>
				<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
			</padding>
		</HBox>
		<TitledPane collapsible="false" maxHeight="1.7976931348623157E308"
			maxWidth="1.7976931348623157E308" text="Past Usernames" VBox.vgrow="ALWAYS">
			<content>
				<ListView onMouseReleased="#onUsernameClicked" fx:id="nameList"
					maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
					prefHeight="100.0" />
			</content>
		</TitledPane>
	</children>
	<padding>
		<Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
	</padding>
</VBox>
